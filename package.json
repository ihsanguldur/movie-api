{
    "name": "movie-api",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "start:dev": "cross-env NODE_ENV=development tsc && cross-env NODE_ENV=development node build/src/main.js",
        "start:test": "cross-env NODE_ENV=test tsc && cross-env NODE_ENV=test node build/src/main.js",
        "start:prod": "cross-env NODE_ENV=production tsc && cross-env NODE_ENV=production node build/src/main.js",
        "test": "jest",
        "lint": "gts lint **/*.ts",
        "clean": "gts clean",
        "compile": "tsc",
        "fix": "gts fix",
        "precommit": "gts lint **/*.ts",
        "prepare": "husky install",
        "pretest": "npm.cmd run compile",
        "posttest": "npm.cmd run lint",
        "typeorm": "ts-node ./node_modules/typeorm/cli",
        "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./src/infra/database/data-source.ts",
        "typeorm:generate-migration": "npm run typeorm -- -d ./src/infra/database/data-source.ts migration:generate ./src/infra/database/migrations/%npm_config_name%",
        "typeorm:create-migration": "npm run typeorm -- migration:create ./src/infra/database/migrations/%npm_config_name%",
        "typeorm:revert-migration": "npm run typeorm -- -d ./src/infra/database/data-source.ts migration:revert"
    },
    "author": "",
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "@types/config": "^3.3.0",
        "@types/connect-timeout": "^0.0.36",
        "@types/debug": "^4.1.7",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.0",
        "@types/morgan": "^1.9.4",
        "@types/node": "^14.11.2",
        "@types/supertest": "^2.0.12",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^5.57.1",
        "@typescript-eslint/parser": "^5.57.1",
        "cross-env": "^7.0.3",
        "eslint": "^8.38.0",
        "gts": "^3.1.1",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "supertest": "^6.3.3",
        "ts-node": "^10.9.1",
        "typescript": "~4.7.0"
    },
    "dependencies": {
        "@types/pino": "^7.0.5",
        "axios": "^1.3.6",
        "config": "^3.3.9",
        "connect-timeout": "^1.9.0",
        "debug": "^4.3.4",
        "express": "^4.18.2",
        "morgan": "^1.10.0",
        "pg": "^8.10.0",
        "pino": "^8.11.0",
        "pino-pretty": "^10.0.0",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^4.6.2",
        "typeorm": "^0.3.15",
        "zod": "^3.21.4"
    }
}
